{
  "name": "@type32/latex2vue",
  "version": "3.1.5",
  "description": "[FORK] Vue components for LaTeX2JS",
  "author": "Dan Lynch <pyramation@gmail.com>, Type-32",
  "homepage": "https://github.com/Type-32/LaTeX2JS",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./esm/index.js",
      "require": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Type-32/LaTeX2JS"
  },
  "keywords": [
    "latex",
    "latex2html",
    "latex2js",
    "math",
    "mathematics",
    "mathjax",
    "vue",
    "vue.js"
  ],
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json src/vue-shims.d.ts dist && copyfiles -u 1 src/latex.vue src/components/**/* dist/ && copyfiles -u 1 src/latex.vue src/components/**/* dist/esm/",
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc && tsc -p tsconfig.esm.json && pnpm run copy",
    "build:dev": "pnpm run clean && tsc --declarationMap && tsc -p tsconfig.esm.json && pnpm run copy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "bugs": {
    "url": "https://github.com/Type-32/LaTeX2JS/issues"
  },
  "devDependencies": {
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "@latex2js/macros": "workspace:^",
    "@latex2js/pstricks": "workspace:^",
    "@latex2js/utils": "workspace:^",
    "latex2js": "^3.1.4",
    "mathjaxjs": "workspace:^",
    "vue": "^3.3.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "gitHead": "53db1d50a534274273c2b1b7cceed210fdc9fdea"
}
